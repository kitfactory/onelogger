# Environment configuration for OneLogger example
# OneLoggerの例用環境設定

LOG_LEVEL=DEBUG
LOG_OUTPUT=both
LOG_FILE_PATH=example.log
LOG_FORMAT=plain
LOG_TIMESTAMP_FORMAT=%Y-%m-%d %H:%M:%S
LOG_ROTATION_TYPE=size
LOG_MAX_FILE_SIZE=10485760
LOG_BACKUP_COUNT=5
LOG_BUFFER_TIME=1
LOG_INCLUDE_PID=true
LOG_INCLUDE_THREAD=true
LOG_APP_NAME=ExampleApp
LOG_STACKTRACE=true
LOG_ASYNC=false
LOG_INCLUDE_SOURCE=true
